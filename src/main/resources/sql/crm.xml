<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="crm">

  <select id="creaSelect" resultType="HashMap">
    select * from crarea
  </select>

  <select id="crasSelect" resultType="HashMap">
    select * from cras
  </select>

  <select id="crdaySelect" resultType="HashMap">
    select * from crday
  </select>

  <select id="creducationalSelect" resultType="HashMap">
    select * from
    creducational
  </select>

  <select id="crhideoutSelect" resultType="HashMap">
    select * from crhideout
  </select>

  <select id="crjobSelect" resultType="HashMap">
    select * from crjob
  </select>

  <select id="crmotiveSelect" resultType="HashMap">
    select * from crmotive
  </select>

  <select id="crtimeSelect" resultType="HashMap">
    select * from crtime
  </select>

  <select id="selectColumnList" parameterType="String"
    resultType="HashMap">
    select `type`, `value`, `column` from column_list where viewNm = #{viewNm}
  </select>

</mapper>